                glancing++;
            }
        }
        crit /= hit_outcomes_mh_.size();
        dodge /= hit_outcomes_mh_.size();
        miss /= hit_outcomes_mh_.size();
        hit /= hit_outcomes_mh_.size();
        glancing /= hit_outcomes_mh_.size();
        glancing += 0.00001;
        crit = 0;
